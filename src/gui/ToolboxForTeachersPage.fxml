<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.ToolboxForTeachersPageController">
   <children>
      <VBox fx:id="parentBox" prefHeight="200.0" prefWidth="100.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <HBox prefHeight="100.0" prefWidth="200.0" VBox.vgrow="ALWAYS">
               <children>
                  <VBox minWidth="220.0" prefHeight="400.0" prefWidth="150.0">
                     <children>
                        <Label text="Moje předměty:">
                           <VBox.margin>
                              <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                           </VBox.margin>
                        </Label>
                        <ListView fx:id="lVMySubjects" minWidth="200.0" prefHeight="353.0" prefWidth="200.0" VBox.vgrow="ALWAYS">
                           <VBox.margin>
                              <Insets bottom="10.0" left="10.0" right="10.0" />
                           </VBox.margin>
                        </ListView>
                     </children>
                  </VBox>
                  <HBox fx:id="hBoxDescription" prefHeight="100.0" prefWidth="200.0" HBox.hgrow="ALWAYS">
                     <children>
                        <VBox minWidth="180.0" prefHeight="400.0" prefWidth="150.0">
                           <children>
                              <Label fx:id="lblGroups" text="Skupiny:" wrapText="true">
                                 <VBox.margin>
                                    <Insets bottom="10.0" right="10.0" top="10.0" />
                                 </VBox.margin>
                              </Label>
                              <ListView fx:id="lVGroups" prefHeight="178.0" prefWidth="180.0" VBox.vgrow="ALWAYS">
                                 <VBox.margin>
                                    <Insets bottom="10.0" />
                                 </VBox.margin>
                              </ListView>
                              <BorderPane prefHeight="290.0" prefWidth="180.0" style="-fx-border-color: black;">
                                 <VBox.margin>
                                    <Insets bottom="10.0" />
                                 </VBox.margin>
                                 <center>
                                    <VBox prefHeight="200.0" prefWidth="100.0" BorderPane.alignment="CENTER">
                                       <children>
                                          <Label fx:id="labelMembers" text="Členové skupiny">
                                             <VBox.margin>
                                                <Insets left="5.0" top="5.0" />
                                             </VBox.margin>
                                          </Label>
                                          <ListView fx:id="lVMembersOfGroup" prefHeight="200.0" prefWidth="200.0" VBox.vgrow="ALWAYS">
                                             <VBox.margin>
                                                <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                                             </VBox.margin>
                                          </ListView>
                                          <Label text="Přidání/odebrání člena skupiny" textAlignment="CENTER" VBox.vgrow="ALWAYS">
                                             <VBox.margin>
                                                <Insets left="10.0" top="5.0" />
                                             </VBox.margin>
                                          </Label>
                                          <ComboBox fx:id="cbUsersToGroup" onAction="#cbUsersToGroupChanged" prefHeight="25.0" prefWidth="564.0" VBox.vgrow="ALWAYS">
                                             <VBox.margin>
                                                <Insets left="10.0" right="10.0" top="5.0" />
                                             </VBox.margin>
                                          </ComboBox>
                                          <Button fx:id="btnAddUser" mnemonicParsing="false" onAction="#btnAddUserToGroup" prefHeight="25.0" prefWidth="907.0" text="Sample" VBox.vgrow="ALWAYS">
                                             <VBox.margin>
                                                <Insets bottom="5.0" left="10.0" right="10.0" top="5.0" />
                                             </VBox.margin>
                                          </Button>
                                       </children>
                                    </VBox>
                                 </center>
                              </BorderPane>
                           </children>
                        </VBox>
                        <VBox minWidth="150.0" prefHeight="400.0" prefWidth="150.0" HBox.hgrow="ALWAYS">
                           <children>
                              <Label fx:id="lblMessages" text="Komentáře ve skupině:" wrapText="true">
                                 <VBox.margin>
                                    <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                                 </VBox.margin>
                              </Label>
                              <ListView fx:id="lVGroupMessages" prefHeight="335.0" prefWidth="180.0" VBox.vgrow="ALWAYS">
                                 <VBox.margin>
                                    <Insets bottom="10.0" left="10.0" right="10.0" />
                                 </VBox.margin>
                              </ListView>
                              <Button mnemonicParsing="false" onAction="#btnChangeClicked" prefHeight="25.0" prefWidth="849.0" text="Upravit" VBox.vgrow="ALWAYS">
                                 <VBox.margin>
                                    <Insets bottom="10.0" left="10.0" right="10.0" />
                                 </VBox.margin>
                              </Button>
                           </children>
                        </VBox>
                     </children>
                  </HBox>
               </children>
            </HBox>
         </children>
      </VBox>
   </children>
</AnchorPane>
